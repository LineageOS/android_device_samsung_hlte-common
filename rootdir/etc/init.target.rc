    chmod 0775 /efs/.files/.dm33
    chmod 0775 /efs/.files/.mp301

# h2k permission
    chmod 0644 /efs/redata.bin
    chown radio radio /efs/h2k.dat
    chmod 0644 /efs/h2k.dat

